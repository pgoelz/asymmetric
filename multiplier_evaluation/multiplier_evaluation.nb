Cell["\<\
The following function takes in the multipliers and the index of an agent. It \
then computes the probability that the largest scaled utility drawn comes \
from the first agent\[CloseCurlyQuote]s distribution.\
\>", "Text",
 CellChangeTimes->{{3.8398825816090384`*^9, 3.839882627263015*^9}, {
  3.840224138676362*^9, 3.8402241395589857`*^9}, {3.851183274450292*^9, 
  3.851183288149345*^9}},ExpressionUUID->"343b5439-7844-46e6-81f1-\
9ebcb127705a"],

Cell[BoxData[
 RowBox[{
  RowBox[{"probmax", "[", 
   RowBox[{"mults_", ",", "i_"}], "]"}], ":=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"PDF", "[", 
       RowBox[{
        RowBox[{"fs", "[", 
         RowBox[{"[", "i", "]"}], "]"}], ",", "x"}], "]"}], "/", 
      RowBox[{"CDF", "[", 
       RowBox[{
        RowBox[{"fs", "[", 
         RowBox[{"[", "i", "]"}], "]"}], ",", "x"}], "]"}]}], "*", 
     RowBox[{"Product", "[", 
      RowBox[{
       RowBox[{"CDF", "[", 
        RowBox[{
         RowBox[{"fs", "[", 
          RowBox[{"[", "j", "]"}], "]"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"mults", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "/", 
           RowBox[{"mults", "[", 
            RowBox[{"[", "j", "]"}], "]"}]}], "*", "x"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"j", ",", "1", ",", "10"}], "}"}]}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "0", ",", "1"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.839881782964818*^9, 3.839881929345948*^9}, {
  3.851163935542759*^9, 3.85116400892562*^9}},
 CellLabel->"In[3]:=",ExpressionUUID->"b33f4ae1-53e7-41e9-b169-f4decbdb97da"],

Cell["\<\
The next function takes in input as above, but computes the expected utility \
conditional on the agent having the largest scaled utility.\
\>", "Text",
 CellChangeTimes->{{3.839882637388679*^9, 
  3.839882673855193*^9}},ExpressionUUID->"59c86cd8-f71b-461b-80f9-\
fdc804cc9727"],

Cell[BoxData[
 RowBox[{
  RowBox[{"condexputil", "[", 
   RowBox[{"mults_", ",", "i_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"x", "*", 
      RowBox[{
       RowBox[{"PDF", "[", 
        RowBox[{
         RowBox[{"fs", "[", 
          RowBox[{"[", "i", "]"}], "]"}], ",", "x"}], "]"}], "/", 
       RowBox[{"CDF", "[", 
        RowBox[{
         RowBox[{"fs", "[", 
          RowBox[{"[", "i", "]"}], "]"}], ",", "x"}], "]"}]}], "*", 
      RowBox[{"Product", "[", 
       RowBox[{
        RowBox[{"CDF", "[", 
         RowBox[{
          RowBox[{"fs", "[", 
           RowBox[{"[", "j", "]"}], "]"}], ",", 
          RowBox[{
           RowBox[{
            RowBox[{"mults", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "/", 
            RowBox[{"mults", "[", 
             RowBox[{"[", "j", "]"}], "]"}]}], "*", "x"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "10"}], "}"}]}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", "0", ",", "1"}], "}"}]}], "]"}], "/", 
   RowBox[{"probmax", "[", 
    RowBox[{"mults", ",", "i"}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.83988197537481*^9, 3.839882027480901*^9}, {
  3.8511641155076733`*^9, 3.851164126444746*^9}, {3.851164929108102*^9, 
  3.851164930563409*^9}},
 CellLabel->"In[4]:=",ExpressionUUID->"1ee081a3-8417-4e10-aa8e-8f2a9fd67f7c"],

