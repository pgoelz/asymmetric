  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"fc", "=", 
   RowBox[{"BetaDistribution", "[", 
    RowBox[{"1", ",", "3"}], "]"}]}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"B", " ", "in", " ", 
    RowBox[{"Fig", ".", " ", "1"}]}], " ", "*)"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"fd", "=", 
   RowBox[{"BetaDistribution", "[", 
    RowBox[{"2", ",", "2"}], "]"}]}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"D", " ", "in", " ", 
    RowBox[{"Fig", ".", " ", "1"}]}], " ", "*)"}], 
  ";"}], "\[IndentingNewLine]", 
